<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Program.
    -->
    <changeSet id="20170429213338-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="school_id"
                                 baseTableName="program"
                                 constraintName="fk_program_school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school"/>
                                 
        <addForeignKeyConstraint baseColumnNames="session_id"
                                 baseTableName="program"
                                 constraintName="fk_session_program_id"
                                 referencedColumnNames="id"
                                 referencedTableName="session"/>

    </changeSet>
</databaseChangeLog>
